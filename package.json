{
  "name": "soap-converter",
  "version": "0.2.1",
  "description": "Instantly convert SOAP/WSDL definitions to REST API specifications for use with Postman, Swagger, and other API development tools.",
  "main": "index.js",
  "bin": "./bin/soap-converter.js",
  "dependencies": {
    "apiconnect-wsdl": "2.0.36",
    "commander": "13.1.0",
    "debug": "4.4.0",
    "inquirer": "12.4.1",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "untildify": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "prettier": "3.4.2"
  },
  "scripts": {
    "lint": "eslint --fix",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
