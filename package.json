  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anhthang/soap-converter.git"
  },
  "bugs": {
    "url": "https://github.com/anhthang/soap-converter/issues"
  },
  "homepage": "https://github.com/anhthang/soap-converter#readme",
  "packageManager": "yarn@4.6.0"
}
